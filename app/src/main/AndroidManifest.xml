<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="heshansandeepa.demo_coordinateexamples">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SimpleCoordinate"
            android:label="@string/title_activity_simple_coordinate"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DetailCoordinate"
            android:label="@string/title_activity_detail_coordinate"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Profile"
            android:label="@string/title_activity_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FlaxibleLayout"
            android:label="@string/title_activity_flaxible_layout"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Swipe"
            android:label="@string/title_activity_swipe"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
